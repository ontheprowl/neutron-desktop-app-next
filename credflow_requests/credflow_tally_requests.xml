                    <XMLTAG>&quot;Instrument_date&quot;</XMLTAG>
                    <SET>$credflow_receipt_instrument_date</SET>
                </FIELD>
                <FIELD NAME="credflow_receipt_transaction_type" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;Transaction_type&quot;</XMLTAG>
                    <SET>$credflow_receipt_transaction_type</SET>
                </FIELD>
                <FIELD NAME="credflow_receipt_instrument_number" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;Instrument_number&quot;</XMLTAG>
                    <SET>$credflow_receipt_instrument_number</SET>
                </FIELD>
                <FIELD NAME="credflow_receipt_bank_name" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;Bank_name&quot;</XMLTAG>
                    <SET>$credflow_receipt_bank_name</SET>
                </FIELD>
                <FIELD NAME="credflow_receipt_payment_mode" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;Payment_mode&quot;</XMLTAG>
                    <SET>$credflow_receipt_payment_mode</SET>
                </FIELD>
                <COLLECTION NAME="CredFlow_datacoll" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <TYPE>vouchers:vouchertype</TYPE>
                    <CHILDOF>$$VchTypeReceipt</CHILDOF>
                    <BELONGSTO>Yes</BELONGSTO>
                    <METHOD>vouchernumber:$vouchernumber</METHOD>
                    <METHOD>credflow_receipt_reference:$reference</METHOD>
                    <METHOD>voucherguid:$guid</METHOD>
                    <METHOD>vchpartyname:$partyledgername</METHOD>
                    <METHOD>vchalterid:$alterid</METHOD>
                    <METHOD>optional : $Isoptional</METHOD>
                    <METHOD>voucherdate:$effectivedate</METHOD>
                    <METHOD>ledgerName:$$FullListEx:&quot;^&quot;:ledgerentries:$ledgername</METHOD>
                    <METHOD>ledgerAmount:$$FullListEx:&quot;^&quot;:ledgerentries:$amount</METHOD>
                    <METHOD>ledgerPositive:$$FullListEx:&quot;^&quot;:ledgerentries:$isdeemedpositive</METHOD>
                    <METHOD>voucher_cancelled:$iscancelled</METHOD>
                    <METHOD>credflow_receipt_instrument_date:$instrumentdate</METHOD>
                    <METHOD>credflow_receipt_transaction_type:$transactiontype</METHOD>
                    <METHOD>credflow_receipt_instrument_number:$instrumentnumber</METHOD>
                    <METHOD>credflow_receipt_bank_name:$bankname</METHOD>
                    <METHOD>credflow_receipt_payment_mode:$paymentmode</METHOD>
                    <NATIVEMETHOD>ledgerentries.*, bankallocations.*</NATIVEMETHOD>
                    <PARMVAR>SVFromDate:Date:$$Date:&quot;01-04-2017&quot;</PARMVAR>
                    <PARMVAR>SVToDate:Date:$$Date:&quot;30-09-2023&quot;</PARMVAR>
                    
                </COLLECTION>
                <COLLECTION NAME="CredFlow_loadedCompaniescoll" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <TYPE>Company</TYPE>
                    <NATIVEMETHOD>Name,Guid</NATIVEMETHOD>
                    <FILTERS>CompanyFilter</FILTERS>
                    <FILTERS>CompanyNameFilter</FILTERS>
                </COLLECTION>
                <COLLECTION NAME="CredFlow_loopReceipts" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <COLLECTIONS>CredFlow_datacoll:CredFlow_loadedCompaniescoll</COLLECTIONS>
                </COLLECTION>
                
                <SYSTEM TYPE="Formulae" NAME="CompanyFilter" ISMODIFY="No" ISFIXED="No" ISINTERNAL="No">$$ExactMatch:($$String:$guid):&quot;9740ced1-ae14-4acc-be43-d3e250e6bf71&quot;   </SYSTEM>
                <SYSTEM TYPE="Formulae" NAME="CompanyNameFilter" ISMODIFY="No" ISFIXED="No" ISINTERNAL="No">$$ExactMatch:($$String:$name):&quot;Test Account&quot;   </SYSTEM>
                <SYSTEM TYPE="Formulae" NAME="AlterIdFilter" ISMODIFY="No" ISFIXED="No" ISINTERNAL="No">$vchalterid &lt; 5000   </SYSTEM>
                
            </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>


