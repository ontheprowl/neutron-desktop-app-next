

1) Company GST Details : [ COMPANYNAME, COMPANYGUID, GST, LASTVOUCHERDATE, NUMBEROFLEDGERS ]


 <ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>CredFlow_CompanyWiseGST</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
            </STATICVARIABLES>
            <TDL>  <TDLMESSAGE>
                <REPORT NAME="CredFlow_CompanyWiseGST" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <FORMS>CredFlow_CompanyWiseGST</FORMS>
                </REPORT>
                <FORM NAME="CredFlow_CompanyWiseGST" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <TOPPARTS>CredFlow_CompanyWiseGST</TOPPARTS>
                </FORM>
                <PART NAME="CredFlow_CompanyWiseGST" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <TOPLINES>CredFlow_CompanyWiseGST</TOPLINES>
                    <REPEAT>CredFlow_CompanyWiseGST : CredFlow_loopTax</REPEAT>
                    <SCROLLED>Vertical</SCROLLED>
                </PART>
                <LINE NAME="CredFlow_CompanyWiseGST" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <LEFTFIELDS>CredFlow_Namecompany, CredFlow_CompanyGUID, CredFlow_GST, CredFlow_LastVoucherDate, CredFlow_numberOfLedgers</LEFTFIELDS>
                    <XMLTAG>&quot;GSTDETAILS&quot;</XMLTAG>
                </LINE>
                <FIELD NAME="CredFlow_CompanyGUID" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;CompanyGuid&quot;</XMLTAG>
                    <SET>$CmpGuid</SET>
                </FIELD>
                <FIELD NAME="CredFlow_LastVoucherDate" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;LastVoucherDate&quot;</XMLTAG>
                    <SET>$lastVoucher</SET>
                </FIELD>
                <FIELD NAME="CredFlow_GST" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;GST&quot;</XMLTAG>
                    <SET>$gst</SET>
                </FIELD>
                <FIELD NAME="CredFlow_Namecompany" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;CompanyName&quot;</XMLTAG>
                    <SET>$CmpName</SET>
                </FIELD>
                <FIELD NAME="CredFlow_numberOfLedgers" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <XMLTAG>&quot;NumberOfLedgers&quot;</XMLTAG>
                    <SET>$numberOfLedgers</SET>
                </FIELD>
                <COLLECTION NAME="CredFlow_datacoll" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <TYPE>TaxUnit</TYPE>
                    <METHOD>gst:$GstRegNumber</METHOD>
                    <METHOD>CmpName:$$LoopCollObj:$Name</METHOD>
                    <METHOD>CmpGuid:$$LoopCollObj:$Guid</METHOD>
                    <METHOD>lastVoucher:$$LoopCollObj:$lastvoucherdate</METHOD>
                    <METHOD>numberOfLedgers:$$LoopCollObj:$Numledgers</METHOD>
                    <NATIVEMETHOD>GstRegNumber</NATIVEMETHOD>
                </COLLECTION>
                <COLLECTION NAME="CredFlow_loadedCompaniescoll" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <TYPE>Company</TYPE>
                    <NATIVEMETHOD>Name,Guid,lastvoucherdate,Numledgers</NATIVEMETHOD>
                        
                        
                </COLLECTION>
                <COLLECTION NAME="CredFlow_loopTax" ISMODIFY="No" ISFIXED="No" ISINITIALIZE="No" ISOPTION="No" ISINTERNAL="No">
                    <COLLECTIONS>CredFlow_datacoll:CredFlow_loadedCompaniescoll</COLLECTIONS>
                </COLLECTION>
                        
                        
            </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>


